@model IEnumerable<WebAppDotNetCoreCrudNew.Models.Student>
@{
    ViewData["Title"] = "getdetails";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h4>Student List</h4>

<p class="float-right">
    <a asp-action="Create" class="btn btn-outline-primary">Add New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>Actions</th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.emp_code)
            </th>*@
            @*<th>
                    @Html.DisplayNameFor(model => model.phonee)
                </th>*@


            <th>
                @Html.DisplayNameFor(model => model.Department)
            </th>



        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
<tr>
    <td>
        @Html.ActionLink("Edit", "Create", new { item.ID, item.Name, item.Email, item.Dep_ID }, new { @class = "btn btn-success" }) |
        @Html.ActionLink("Delete", "DeleteStd", new { item.ID }, new { @class = "btn btn-danger", onclick = "return confirm('Do you really want this record?')" })
    </td>


    <td>
        @Html.DisplayFor(modelItem => item.Name)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.Email)
    </td>

    @*<td>
        @Html.DisplayFor(modelItem => item.emp_code)
    </td>*@

    @*<td>
            @Html.DisplayFor(modelItem => item.phonee)
        </td>*@

    <td>
        @Html.DisplayFor(modelItem => item.Department)
    </td>



</tr>
}
    </tbody>
</table>




